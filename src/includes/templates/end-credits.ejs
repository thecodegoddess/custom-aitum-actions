<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>End Credits</title>
  <link href="https://fonts.googleapis.com" rel="preconnect">
  <link crossorigin href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css2?family=Lilita+One&display=swap" rel="stylesheet">
  <link href="end-credits.css" rel="stylesheet">
</head>
<body>
<main>
  <section class="end-credits" style="--credits-length:<%= creditsLength -%>">
    <h2 class="end-credits__title">Thank you to all these amazing people for chatting with us</h2>
      <div class="end-credits__window">
        <ul class="end-credits__list">
          <%_ credits.forEach(function(name) { -%>
            <li class="end-credits__name"><span><%= name -%></span></li>
          <%_ }) -%>
            <li class="end-credits__name"><span>Thank You!</span></li>
        </ul>
      </div>

  </section>
</main>
</body>
</html>
